{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,

    "module": "commonjs",
    "target": "es6",
    "lib": [
      "es2015",
      "es2017.object",
      "dom"
    ],
    "allowSyntheticDefaultImports": true,
    "jsx": "react"
  }
}
